\mode<presentation>

\definecolor{turquoise}{rgb}{0, 0.81, 0.87}
%\definecolor{lightturquoise}{rgb}{0.25, 0.88, 0.82}
\definecolor{aqua}{rgb}{0.498, 1, 0.831}

\setbeamercolor{background canvas}{bg=aqua!10!white} %sfondo

\setbeamercolor{normal text}{bg=aqua!10!white,fg=black}%ombra dei blocchi; colore del testo principale

\setbeamercolor{section in toc}{fg=black,bg=white}%non so
\setbeamercolor{alerted text}{fg=red!80!gray}%non so
\setbeamercolor*{palette primary}{fg=turquoise!60!black,bg=turquoise!30!white}%testo e sfondo data e numero di frame
\setbeamercolor*{palette secondary}{fg=turquoise!70!black,bg=turquoise!15!white}%testo e sfondo titolo tesi in basso
\setbeamercolor*{palette tertiary}{bg=turquoise!80!black,fg=turquoise!10!white}%sfondo e testo nome e cognome e sezione
\setbeamercolor*{palette quaternary}{fg=turquoise,bg=turquoise!5!white}%non so

\setbeamercolor*{sidebar}{fg=turquoise,bg=gray!15!white}%non so


\setbeamercolor*{palette sidebar primary}{fg=turquoise!10!black}
\setbeamercolor*{palette sidebar secondary}{fg=white}
\setbeamercolor*{palette sidebar tertiary}{fg=turquoise!50!black}
\setbeamercolor*{palette sidebar quaternary}{fg=gray!10!white}

\setbeamercolor*{titlelike}{parent=palette primary}%non so, ma sembra parte del comando successivo
\setbeamercolor{titlelike}{parent=palette primary,fg=turquoise!75!black}%copia lo stile palette primary ma cambia colore titolo e titoli dei frames
\setbeamercolor{frametitle}{bg=turquoise!10!white}%cambia lo sfondo dei titoli frame
\setbeamercolor{frametitle right}{bg=turquoise!60!white}%non so

\setbeamercolor{title}{bg=turquoise!15!white}%cambia lo sfondo del titolo e dei titoli dei blocchi

\setbeamercolor*{block title}{parent=palette primary,fg=black}%non so, titolo blocchi
\setbeamercolor*{block title alerted}{fg=black,bg=black!20}
\setbeamercolor*{block title example}{fg=black,bg=black!20}

\setbeamercolor*{block body}{parent=palette secondary,fg=black}%copia lo stile palette secondary ma cambia colore testo blocchi
\setbeamercolor*{block body alerted}{fg=black,bg=black!10}
\setbeamercolor*{block body example}{fg=black,bg=black!10}

\setbeamercolor*{separation line}{}
\setbeamercolor*{fine separation line}{}

\mode
<all>
